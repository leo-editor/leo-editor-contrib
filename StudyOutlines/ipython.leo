<t tx="ekr.20050704165326.14">def remove(self,num):
    """Remove a finished (completed or dead) job."""

    try:
        job = self.jobs_all[num]
    except KeyError:
        error('Job #%s not found' % num)
    else:
        stat_code = job.stat_code
        if stat_code == self._s_running:
