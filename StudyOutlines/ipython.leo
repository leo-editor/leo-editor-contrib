    VTable_ptr = POINTER("%s_VTable" % name)
    fields = [("lpVtbl", VTable_ptr)]
    kwds["_fields_"] = fields
    result = type(Structure).__new__(cls, name, bases, kwds)
    result.VTable_ptr = VTable_ptr
    if kwds.has_key("_methods_"):
