<t tx="ekr.20050704165326.604">def text(self,etype=None,value=None,tb=None,context=5,mode=None):
    if etype is None:
        etype,value,tb = sys.exc_info()
    self.tb = tb
    return FormattedTB.text(self,etype,value,tb,context=5,mode=mode)
