def QueryInterface(self, this, refiid, ppiunk):
    iid = refiid[0]
    for i, itf in self._com_pointers_:
        if i == iid:
            # *ppiunk = &amp;itf
            return CopyComPointer(addressof(itf), ppiunk)
    return E_NOINTERFACE
