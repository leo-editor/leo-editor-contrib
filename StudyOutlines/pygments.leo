            (r'(&lt;-|::|-&gt;|=&gt;|=)(?![:!#$%&amp;*+.\\/&lt;=&gt;?@^|~-]+)', Operator.Word),  # specials
            (r':[:!#$%&amp;*+.\\/&lt;=&gt;?@^|~-]*', Keyword.Type),  # Constructor operators
            (r'[:!#$%&amp;*+.\\/&lt;=&gt;?@^|~-]+', Operator),  # Other operators
