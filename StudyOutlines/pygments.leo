                u'.', u'.=', u'.!', u'&lt;:', u'&gt;:', u'', u'', u'', u'', u'',
                u'', u'',
                u'', u'',
                # prec-pipe
                u'|&gt;', u'&lt;|',
                # prec-colon
                u':',
                # prec-plus
                u'+', u'-', u'.+', u'.-', u'|', u'', u'$',
                # prec-bitshift
                u'&lt;&lt;', u'&gt;&gt;', u'&gt;&gt;&gt;', u'.&lt;&lt;', u'.&gt;&gt;', u'.&gt;&gt;&gt;',
                # prec-times
                u'*', u'/', u'./', u'', u'.', u'%', u'', u'.%', u'.*', u'\\', u'.\\', u'&amp;', u'',
