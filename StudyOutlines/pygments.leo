<t tx="ekr.20190320063830.337">def _closest_color(self, r, g, b):
    distance = 257*257*3  # "infinity" (&gt;distance from #000000 to #ffffff)
    match = 0

    for i in range(0, 254):
        values = self.xterm_colors[i]

        rd = r - values[0]
        gd = g - values[1]
        bd = b - values[2]
        d = rd*rd + gd*gd + bd*bd

        if d &lt; distance:
            match = i
            distance = d
    return match

