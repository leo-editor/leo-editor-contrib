    #------ Alt modifier ---------------------------------------------------

    elif alt_down:
        if key == QtCore.Qt.Key_B:
            self._set_cursor(self._get_word_start_cursor(position))
            intercepted = True

        elif key == QtCore.Qt.Key_F:
            self._set_cursor(self._get_word_end_cursor(position))
            intercepted = True

        elif key == QtCore.Qt.Key_Y:
            self._kill_ring.rotate()
