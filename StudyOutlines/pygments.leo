<t tx="ekr.20190320063830.138">def __getattr__(self, val):
    if not val or not val[0].isupper():
        return tuple.__getattribute__(self, val)
    new = _TokenType(self + (val,))
    setattr(self, val, new)
    self.subtypes.add(new)
    new.parent = self
    return new

