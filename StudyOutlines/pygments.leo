                                  name+'-string'))
        states[name+'-regex'] = [
            (r'\\[\\' + bracecc + ']', String.Regex),
            (lbrace, String.Regex, '#push'),
            (rbrace + '[mixounse]*', String.Regex, '#pop'),
            include('string-intp'),
            (r'[\\#' + bracecc + ']', String.Regex),
            (r'[^\\#' + bracecc + ']+', String.Regex),
