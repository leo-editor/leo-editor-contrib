    aliases = ['rbcon', 'irb']
    mimetypes = ['text/x-ruby-shellsession']

    _prompt_re = re.compile('irb\([a-zA-Z_]\w*\):\d{3}:\d+[&gt;*"\'] '
                            '|&gt;&gt; |\?&gt; ')

    @others
