<t tx="ekr.20190320063830.1274">def analyse_text(text):
    rv = 0.0
    if re.search('\{if\s+.*?\}.*?\{/if\}', text):
        rv += 0.15
    if re.search('\{include\s+file=.*?\}', text):
        rv += 0.15
    if re.search('\{foreach\s+.*?\}.*?\{/foreach\}', text):
