            (r'\.\.', Punctuation),
            # Opening Chevron Bracket
            (r'&lt;&lt;', Punctuation),  # M2R10 + ISO
            # Closing Chevron Bracket
            (r'&gt;&gt;', Punctuation),  # M2R10 + ISO
            # Blueprint Punctuation
            (r'-&gt;', Punctuation),  # M2R10 + ISO
            # Distinguish |# and # in M2 R10
            (r'\|#', Punctuation),
            # Distinguish ## and # in M2 R10
