<t tx="ekr.20190320063830.914">def analyse_text(text):
    rv = 0.0
    # TODO: let/in
    if re.search(r'import.+?&lt;[^&gt;]+&gt;', text):
        rv += 0.4
    if re.search(r'mkDerivation\s+(\(|\{|rec)', text):
        rv += 0.4
    if re.search(r'=\s+mkIf\s+', text):
        rv += 0.4
    if re.search(r'\{[a-zA-Z,\s]+\}:', text):
        rv += 0.1
    return rv
