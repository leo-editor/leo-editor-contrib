<t tx="ekr.20190320063830.1095">def _split_from_pipes(self, row):
    _, separator, rest = self._pipe_splitter.split(row, 1)
    yield separator
    while self._pipe_splitter.search(rest):
        cell, separator, rest = self._pipe_splitter.split(rest, 1)
