        # Macro parameters
        (r'(\?' + valid_name + ')(:)'
         r'(token|name|variable|expression|body|case-body|\*)',
            bygroups(Name.Tag, Operator, Name.Builtin)),
        (r'(\?)(:)(token|name|variable|expression|body|case-body|\*)',
            bygroups(Name.Tag, Operator, Name.Builtin)),
        (r'\?' + valid_name, Name.Tag),

        # Punctuation
        (r'(=&gt;|::|#\(|#\[|##|\?\?|\?=|\?|[(){}\[\],.;])', Punctuation),

