        (r'"', String.Double, 'string'),
        (r'~?0x[0-9a-fA-F]+', Number.Hex),
        (r'0wx[0-9a-fA-F]+', Number.Hex),
        (r'0w\d+', Number.Integer),
        (r'~?\d+\.\d+[eE]~?\d+', Number.Float),
        (r'~?\d+\.\d+', Number.Float),
        (r'~?\d+[eE]~?\d+', Number.Float),
        (r'~?\d+', Number.Integer),

