<t tx="ekr.20190320063830.1091">def _tokenize(self, value, index, commented, separator, heading):
    if commented:
        yield value, COMMENT
    elif separator:
        yield value, SEPARATOR
    elif heading:
        yield value, HEADING
    else:
        for value, token in self._table.tokenize(value, index):
            yield value, token


