<t tx="ekr.20190320063830.207">def filter(self, lexer, stream):
    current_type = None
    current_value = None
    for ttype, value in stream:
        if ttype is current_type:
            current_value += value
        else:
            if current_type is not None:
                yield current_type, current_value
            current_type = ttype
            current_value = value
    if current_type is not None:
        yield current_type, current_value


