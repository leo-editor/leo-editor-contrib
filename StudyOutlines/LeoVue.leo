  if (val === null || val.toString() === 'NaN') val = null;
  if (val === null) return '&lt;'+ name +' d:constr="null"/&gt;';
  if (val.length === 1 &amp;&amp; val.constructor === Array &amp;&amp; !val[0]) {
    return '&lt;'+ name +' d:constr="null" d:type="ArrayItem"/&gt;';
  }
