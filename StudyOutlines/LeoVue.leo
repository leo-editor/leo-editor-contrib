    text = (text !== null)? text[2] : '';

    return '&lt;'+ name + a1 +' '+ a2 +' d:type="ArrayItem"&gt;'+ text +'&lt;/'+ name +'&gt;';
  } else if (val.length === 0 &amp;&amp; val.constructor === Array) {
    return '&lt;'+ name +' d:constr="Array"/&gt;';
