           &lt;div class="split-right"
                v-show="showRightButton"
                @click="slide('right')"&gt;&lt;icon name="angle-double-right"&gt;&lt;/icon&gt;
           &lt;/div&gt;
      &lt;/div&gt;
      &lt;div id="right-pane" class="right-pane"&gt;
