<t tx="ekr.20040104185913.1512"># Python simulation of weightedUnion.
def _wunion(self, A, B, w1=1, w2=1):
    if isaset(A) and isaset(B):
        return 1, self.union(A, B).keys()
    A = self._normalize(A)
    B = self._normalize(B)
    result = []
    for key in self.union(A, B):
        v1 = A.get(key, 0)
        v2 = B.get(key, 0)
        result.append((key, v1*w1 + v2*w2))
    return 1, result
