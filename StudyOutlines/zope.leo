	  if (AsExtensionClass(base)-&gt;bases)
	    {
	      if ((dbase=datafull_baseclass(AsExtensionClass(base))))
		return dbase;
	    }
	  else
	    {
	      if (AsExtensionClass(base)-&gt;tp_basicsize &gt;
		 sizeof(PyPureMixinObject) ||
		 AsExtensionClass(base)-&gt;tp_itemsize &gt; 0)
		return base;
	    }
	}
    }
  return NULL;
}
