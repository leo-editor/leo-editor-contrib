  else
    i= BTree_index( BTREE(self-&gt;data[min].value), key, less);

  if(i==-9) goto err;

  while(--min &gt;= 0) i+=self-&gt;data[min].count;

  PER_ALLOW_DEACTIVATION(self);
  return i;

err:

  PER_ALLOW_DEACTIVATION(self);
  return -9;
