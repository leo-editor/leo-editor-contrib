def plain_replace(a, alo, ahi, b, blo, bhi):
    assert alo &lt; ahi and blo &lt; bhi
    # dump the shorter block first -- reduces the burden on short-term
    # memory if the blocks are of very different sizes
    if bhi - blo &lt; ahi - alo:
