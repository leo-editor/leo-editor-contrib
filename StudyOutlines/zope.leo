    l=key.rfind('-')
    if l &lt; 0:
        alt_prefix = self.alt_prefix
        if not (alt_prefix and key.startswith(alt_prefix)):
            raise KeyError, key

        suffix = key[len(alt_prefix):].replace('_', '-')
        if '-' in suffix:
