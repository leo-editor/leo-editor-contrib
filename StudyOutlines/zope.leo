<t tx="ekr.20040104185913.536">def _request_debug(self,res,expected,args,kw):

    s = 'Args: %s' % str(args)
    s+= ', KW: %s' % str(kw)
    s+= '\n%s\n' % res.__str__(with_output=0,expected=expected)

