        data='\n'.join(data.split('\n\r'))

    if self.edited_source:
        self.edited_source=data
        self._v_cooked=self.cook()
    else:
        __traceback_info__=self.__class__
        newHTML=self.__class__()
        newHTML.__setstate__(self.__getstate__())
