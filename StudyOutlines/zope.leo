InstanceDict___init__(InstanceDictobject *self, PyObject *args)
{
  self-&gt;guarded_getattr=NULL;
  UNLESS(PyArg_ParseTuple(args, "OO|O",
			  &amp;(self-&gt;inst),
			  &amp;(self-&gt;namespace),
