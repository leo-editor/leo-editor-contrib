static int
Record_compare(Record *v, Record *w)
{
  int lv, lw, i, c;
  PyObject **dv, **dw;

  if((lv=Record_init(v)) &lt; 0) return -1;
  if((lw=Record_init(w)) &lt; 0) return -1;
  if(lw &lt; lv) lv=lw;

  for(i=0, dv=v-&gt;data, dw=w-&gt;data; i &lt; lv; i++, dv++, dw++)
    {
      if(*dv)
	if(*dw)
	  {
	    if((c=PyObject_Compare(*dv,*dw))) return c;
	  }
	else return 1;
      else if(*dw) return -1;
    }
  if(*dv) return 1;
  if(*dw) return -1;
  return 0;
}
