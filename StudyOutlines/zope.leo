    title=str(title)
    module=str(module)
    function=str(function)

    self.title=title
    if module[-3:]=='.py': module=module[:-3]
    elif module[-4:]=='.pyc': module=module[:-4]
    self._module=module
    self._function=function
    self.getFunction(1)
