<t tx="ekr.20040104185913.738">def rcdeltas(self):
    if _v_rcs is None:
        self.rcsnapshot()
    nc=self.refdict()
    rc=_v_rcs
    rd=[]
    for n, c in nc.items():
        try:
            prev=rc[n]
            if c &gt; prev:
                rd.append( (c - prev, (c, prev, n)) )
        except: pass
    rd.sort()
    rd.reverse()
    return map(lambda n: {'name': n[1][2],
                          'delta': n[0],
                          'pc': n[1][1],
                          'rc': n[1][0]}, rd)
