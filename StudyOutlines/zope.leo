  UNLESS(self-&gt;data) return PyTuple_New(0);

  if((l=Record_init(self)) &lt; 0) return NULL;
  UNLESS(r=PyTuple_New(l)) return NULL;
  for(d=self-&gt;data, i=0; i &lt; l; i++, d++)
    {
      v= *d;
