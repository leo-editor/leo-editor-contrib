<t tx="ekr.20040104185913.4160">def test_retrieval( self ):

    work = DateRangeIndex( 'work', 'start', 'stop' )

    for i in range( len( dummies ) ):
        work.index_object( i, dummies[i] )

    for i in range( len( dummies ) ):
        assert work.getEntryForObject( i ) == dummies[i].datum()

