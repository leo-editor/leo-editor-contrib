def _wintersection(self, A, B, w1=1, w2=1):
    if isaset(A) and isaset(B):
        return w1 + w2, self.intersection(A, B).keys()
    A = self._normalize(A)
    B = self._normalize(B)
    result = []
    for key in self.intersection(A, B):
