static int
load_dict(Unpicklerobject *self) {
    PyObject *dict, *key, *value;
    int i, j, k;

    if ((i = marker(self)) &lt; 0) return -1;
    j=self-&gt;stack-&gt;length;

    UNLESS (dict = PyDict_New()) return -1;

    for (k = i+1; k &lt; j; k += 2) {
        key  =self-&gt;stack-&gt;data[k-1];
