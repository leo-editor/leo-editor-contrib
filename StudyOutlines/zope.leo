                '\.(?P&lt;minor&gt;[0-9]+)\.(?P&lt;micro&gt;[0-9]+)'
                '(?P&lt;status&gt;[A-Za-z]+)?(?P&lt;release&gt;[0-9]+)?')
            dict = expr.match(s).groupdict()
            _zope_version = (
                intval(dict, 'major'),
                intval(dict, 'minor'),
                intval(dict, 'micro'),
                strval(dict, 'status'),
                intval(dict, 'release'))
        except:
            ss = 'unreleased version, python %d.%d.%d, %s' % (
                v[0],v[1],v[2],sys.platform)
            _version_string = ss
