  int l, i;
  PyObject *base;
  
  l=PyTuple_Size(type-&gt;bases);
  for (i=0; i &lt; l &amp;&amp; ! (*c1 &amp;&amp; *c2); i++)
    {
      base=PyTuple_GET_ITEM(type-&gt;bases, i);
      if (ExtensionClass_Check(base))
