<t tx="ekr.20040104185913.4142">def _convertDateTime( self, value ):
    if value is None:
        return value
    if type( value ) == type( '' ):
        dt_obj = DateTime( value )
        value = dt_obj.millis() / 1000 / 60 # flatten to minutes
    if isinstance( value, DateTime ):
