<t tx="ekr.20181108111640.253">def _iterator_assign(self, val, *names):
    if len(names) == 1:
        return '%s = %s;' % (names[0], val)
    else:
        code = []
        for i, name in enumerate(names):
            code.append('%s = %s[%i];' % (name, val, i))
