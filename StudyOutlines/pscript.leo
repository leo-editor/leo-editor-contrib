<t tx="ekr.20181108111640.186">def get_declarations(self, ns):
    """ Get string with variable (and builtin-function) declarations.
    """
    if not ns:
        return ''
    code = []
    loose_vars = []
    for name, value in sorted(ns.items()):
        if value == 1:
            loose_vars.append(name)
