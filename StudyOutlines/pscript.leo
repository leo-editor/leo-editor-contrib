<t tx="ekr.20181108111640.87">def _convert_Num(self, n):
    if pyversion &lt; (3, ) and str(n.n).startswith('-'):
        # -4 is a unary sub on 4, dont forget complex numbers
        return UnaryOp(Node.OPS.USub, Num(-n.n))
    return Num(n.n)

