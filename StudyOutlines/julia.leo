<t tx="ekr.20120404123906.8047">static jl_value_t *build_backtrace(void)
{
    void *array[1024];
    size_t ip;
    size_t *p;
    jl_array_t *a;
    a = jl_alloc_cell_1d(0);
    JL_GC_PUSH(&amp;a);
    
    backtrace(array, 1023);
    p = (size_t*)array;
    while ((ip = *(p++)) != 0) {
        push_frame_info_from_ip(a, ip);
    }
    JL_GC_POP();
    return (jl_value_t*)a;
}
