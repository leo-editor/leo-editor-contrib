{
    return ((jl_is_symbol(e) &amp;&amp; ((jl_sym_t*)e)==sym) ||
            (jl_is_symbolnode(e) &amp;&amp; jl_symbolnode_sym(e)==sym) ||
            (jl_is_topnode(e) &amp;&amp; ((jl_sym_t*)jl_fieldref(e,0))==sym));
}
