    if (i&amp;1) i++;
    while (i &lt; t-&gt;length &amp;&amp; ((void**)t-&gt;data)[i+1] == NULL)
        i+=2;
    if (i &gt;= t-&gt;length) return (jl_value_t*)jl_null;
