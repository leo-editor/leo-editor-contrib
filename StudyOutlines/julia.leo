<t tx="ekr.20120404123906.7767">jl_function_t *jl_instantiate_method(jl_function_t *f, jl_tuple_t *sp)
{
    if (f-&gt;linfo == NULL)
        return f;
    jl_function_t *nf = jl_new_closure(f-&gt;fptr, f-&gt;env, NULL);
    JL_GC_PUSH(&amp;nf);
    nf-&gt;linfo = jl_add_static_parameters(f-&gt;linfo, sp);
    JL_GC_POP();
    return nf;
}
