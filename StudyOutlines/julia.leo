<t tx="ekr.20120404123906.7727">static void gc_sweep(void)
{
    sweep_big();
    int i;
    for(i=0; i &lt; N_POOLS; i++) {
        sweep_pool(&amp;norm_pools[i]);
        sweep_pool(&amp;ephe_pools[i]);
    }
    jl_unmark_symbols();
}
