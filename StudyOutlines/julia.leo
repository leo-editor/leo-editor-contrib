<t tx="ekr.20120404123906.7779">
static int tuple_all_Any(jl_tuple_t *t)
{
    int i;
    for(i=0; i &lt; t-&gt;length; i++) {
        if (jl_tupleref(t,i) != (jl_value_t*)jl_any_type)
            return 0;
    }
    return 1;
}
