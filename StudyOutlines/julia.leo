<t tx="ekr.20120404123906.7784">static int sigs_eq(jl_value_t *a, jl_value_t *b)
{
    if (jl_has_typevars(a) || jl_has_typevars(b)) {
        return jl_types_equal_generic(a,b);
    }
    return jl_subtype(a, b, 0) &amp;&amp; jl_subtype(b, a, 0);
}
