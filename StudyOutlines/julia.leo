{
    if (!jl_is_expr(ex)) return 0;
    if (((jl_expr_t*)ex)-&gt;head != colons_sym) return 0;
    jl_expr_t *atype = (jl_expr_t*)jl_exprarg(ex,1);
    if (!jl_is_expr(atype)) return 0;
    if (atype-&gt;head != call_sym || atype-&gt;args-&gt;length != 3)
