static void cache_type_(jl_type_t *type)
{
    // only cache concrete types
    jl_tuple_t *t = ((jl_tag_type_t*)type)-&gt;parameters;
    if (t-&gt;length == 0) return;
    if (jl_is_tag_type(type)) {
        if (jl_has_typevars_((jl_value_t*)type,1))
            return;
