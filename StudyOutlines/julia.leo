<t tx="ekr.20120404123906.7642">
static void show_function(jl_value_t *v)
{
    ios_t *s = jl_current_output_stream();
    if (jl_is_gf(v)) {
        ios_puts(jl_gf_name(v)-&gt;name, s);
    }
    else {
        ios_puts("#&lt;function&gt;", s);
    }
}
