static int very_general_type(jl_value_t *t)
{
    return (t &amp;&amp; (t==(jl_value_t*)jl_any_type ||
                  (jl_is_typevar(t) &amp;&amp;
                   ((jl_tvar_t*)t)-&gt;ub==(jl_value_t*)jl_any_type)));
