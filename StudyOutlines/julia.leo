<t tx="ekr.20120404123906.8155">static char *type_summary(jl_value_t *t)
{
    if (jl_is_tuple(t)) return "Tuple";
    if (jl_is_some_tag_type(t))
        return ((jl_tag_type_t*)t)-&gt;name-&gt;name-&gt;name;
    return "?";
}
