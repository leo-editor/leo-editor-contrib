    jl_current_task-&gt;state.eh_ctx = ss-&gt;eh_ctx;
    jl_current_task-&gt;state.bt = ss-&gt;bt;
    jl_current_task-&gt;state.ostream_obj = ss-&gt;ostream_obj;
