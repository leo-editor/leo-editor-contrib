static jl_value_t *switchto(jl_task_t *t)
{
    if (t-&gt;done==jl_true) {
        jl_task_arg_in_transit = (jl_value_t*)jl_null;
        return t-&gt;result;
    }
    ctx_switch(t, &amp;t-&gt;ctx);
    jl_value_t *val = jl_task_arg_in_transit;
    jl_task_arg_in_transit = (jl_value_t*)jl_null;
    return val;
}
