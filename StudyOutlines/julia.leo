<t tx="ekr.20120404123906.7406">
static Function *jlfunc_to_llvm(const std::string &amp;cname, void *addr)
{
    Function *f =
        Function::Create(jl_func_sig, Function::ExternalLinkage,
                         cname, jl_Module);
    jl_ExecutionEngine-&gt;addGlobalMapping(f, addr);
    return f;
}
