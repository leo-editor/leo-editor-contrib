<t tx="ekr.20120404123906.7655">
JL_CALLABLE(jl_f_new_tag_type_super)
{
    JL_NARGS(new_tag_type_super, 2, 2);
    JL_TYPECHK(new_tag_type_super, tag_type, args[1]);
