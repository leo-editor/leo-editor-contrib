static Value *uint_cnvt(Type *to, Value *x)
{
    Type *t = x-&gt;getType();
    if (t == to) return x;
    if (to-&gt;getPrimitiveSizeInBits() &lt; x-&gt;getType()-&gt;getPrimitiveSizeInBits())
        return builder.CreateTrunc(x, to);
