<t tx="398">static void
_listreverse(PyListObject *self)
{
	register PyObject **p, **q;
	register PyObject *tmp;
	
	if (self-&gt;ob_size &gt; 1) {
		for (p = self-&gt;ob_item, q = self-&gt;ob_item + self-&gt;ob_size - 1;
		     p &lt; q;
		     p++, q--)
		{
			tmp = *p;
			*p = *q;
			*q = tmp;
		}
	}
}
