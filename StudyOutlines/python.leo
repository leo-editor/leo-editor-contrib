<t tx="1130">static void
com_subscript(struct compiling *c, node *n)
{
	node *ch;
	REQ(n, subscript);
	ch = CHILD(n,0);
	/* check for rubber index */
	if (TYPE(ch) == DOT &amp;&amp; TYPE(CHILD(n,1)) == DOT) {
