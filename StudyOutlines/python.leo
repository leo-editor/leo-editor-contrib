	if (c-&gt;c_nblocks &gt; 0)
		c-&gt;c_nblocks--;
	if (c-&gt;c_block[c-&gt;c_nblocks] != type &amp;&amp; c-&gt;c_errors == 0) {
		com_error(c, PyExc_SystemError, "bad block pop");
	}
}
