	PyObject *self;
	static PyObject *not_yet_string;

	assert(type != NULL &amp;&amp; type-&gt;tp_alloc != NULL);

	if (not_yet_string == NULL) {
		not_yet_string = PyString_FromString("&lt;uninitialized file&gt;");
		if (not_yet_string == NULL)
