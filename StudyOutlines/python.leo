			if (fill == ' ') {
				if (sign)
					*res++ = sign;
				if ((flags &amp; F_ALT) &amp;&amp;
				    (c == 'x' || c == 'X')) {
					assert(pbuf[0] == '0');
					assert(pbuf[1] == c);
					*res++ = *pbuf++;
					*res++ = *pbuf++;
				}
