	if (last &lt; 0)
		last += len;
	if (last &lt; 0)
		last = 0;
	if (i &lt; 0)
		i += len;
	if (i &lt; 0)
		i = 0;

	if (dir &gt; 0) {
		if (n == 0 &amp;&amp; i &lt;= last)
			return (long)i;
		last -= n;
