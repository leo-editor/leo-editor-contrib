<t tx="1165">static void
com_raise_stmt(struct compiling *c, node *n)
{
	int i;
	REQ(n, raise_stmt); /* 'raise' [test [',' test [',' test]]] */
	if (NCH(n) &gt; 1) {
		com_node(c, CHILD(n, 1));
		if (NCH(n) &gt; 3) {
			com_node(c, CHILD(n, 3));
			if (NCH(n) &gt; 5)
				com_node(c, CHILD(n, 5));
		}
	}
	i = NCH(n)/2;
	com_addoparg(c, RAISE_VARARGS, i);
	com_pop(c, i);
}
