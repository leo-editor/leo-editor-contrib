<t tx="1088">static void
dump(node *n, int pad, int depth)
{
	int i;
	if (depth == 0)
	    return;
	do_pad(pad);
	fprintf(stderr, "%d: %s\n", TYPE(n), STR(n));
	if (depth &gt; 0)
	    depth--;
	for (i = 0; i &lt; NCH(n); ++i)
		dump(CHILD(n, i), pad + 1, depth);
