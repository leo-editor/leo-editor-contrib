		goto Fail;

	for (;;) {
		PyObject *op2;

		if (args-&gt;ob_refcnt &gt; 1) {
			Py_DECREF(args);
			if ((args = PyTuple_New(2)) == NULL)
				goto Fail;
		}

		op2 = PyIter_Next(it);
		if (op2 == NULL) {
			if (PyErr_Occurred())
				goto Fail;
