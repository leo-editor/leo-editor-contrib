		Py_INCREF(not_yet_string);
		((PyFileObject *)self)-&gt;f_name = not_yet_string;
		Py_INCREF(not_yet_string);
		((PyFileObject *)self)-&gt;f_mode = not_yet_string;
	}
	return self;
}
