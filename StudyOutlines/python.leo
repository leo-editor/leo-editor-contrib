				return (long)i;
	}
	else {
		int j;

        	if (n == 0 &amp;&amp; i &lt;= last)
			return (long)last;
		for (j = last-n; j &gt;= i; --j)
			if (s[j] == sub[0] &amp;&amp; memcmp(&amp;s[j], sub, n) == 0)
				return (long)j;
	}

