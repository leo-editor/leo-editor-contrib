static PyObject *
builtin_round(PyObject *self, PyObject *args)
{
	double x;
	double f;
	int ndigits = 0;
	int i;

	if (!PyArg_ParseTuple(args, "d|i:round", &amp;x, &amp;ndigits))
			return NULL;
	f = 1.0;
	i = abs(ndigits);
	while  (--i &gt;= 0)
		f = f*10.0;
	if (ndigits &lt; 0)
		x /= f;
	else
		x *= f;
	if (x &gt;= 0.0)
		x = floor(x + 0.5);
	else
		x = ceil(x - 0.5);
	if (ndigits &lt; 0)
		x *= f;
	else
		x /= f;
	return PyFloat_FromDouble(x);
}
