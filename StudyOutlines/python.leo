builtin_apply(PyObject *self, PyObject *args)
{
	PyObject *func, *alist = NULL, *kwdict = NULL;
	PyObject *t = NULL, *retval = NULL;

	if (!PyArg_ParseTuple(args, "O|OO:apply", &amp;func, &amp;alist, &amp;kwdict))
		return NULL;
	if (alist != NULL) {
		if (!PyTuple_Check(alist)) {
