def findlabels(code):
    """Detect all offsets in a byte code which are jump targets.

    Return the list of offsets.

    """
    labels = []
    n = len(code)
    i = 0
    while i &lt; n:
        c = code[i]
        op = ord(c)
        i = i+1
        if op &gt;= HAVE_ARGUMENT:
            oparg = ord(code[i]) + ord(code[i+1])*256
            i = i+2
            label = -1
            if op in hasjrel:
                label = i+oparg
            elif op in hasjabs:
                label = oparg
            if label &gt;= 0:
                if label not in labels:
                    labels.append(label)
    return labels
