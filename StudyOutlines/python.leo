	if (!changed &amp;&amp; PyString_CheckExact(input_obj)) {
		Py_DECREF(result);
		Py_INCREF(input_obj);
		return input_obj;
	}
	/* Fix the size of the resulting string */
	if (inlen &gt; 0 &amp;&amp;_PyString_Resize(&amp;result, output-output_start))
		return NULL;
