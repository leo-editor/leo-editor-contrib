		PyErr_SetString(PyExc_TypeError,
				"expected a single-segment buffer object");
		return -1;
	}
	len = (*pb-&gt;bf_getwritebuffer)(obj,0,&amp;pp);
	if (len &lt; 0)
		return -1;
	*buffer = pp;
	*buffer_len = len;
	return 0;
